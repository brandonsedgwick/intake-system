# Authentication
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your-nextauth-secret-generate-with-openssl-rand-base64-32

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Google Sheets
GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id
GOOGLE_FORM_RESPONSES_SHEET_ID=your-form-responses-sheet-id

# Gmail Pub/Sub (for real-time email notifications)
GMAIL_PUBSUB_TOPIC=projects/your-project/topics/gmail-push

# Encryption (generate with: openssl rand -hex 32)
ENCRYPTION_KEY=your-256-bit-hex-key-for-aes-encryption

# Simple Practice Credentials (for Puppeteer automation)
SIMPLE_PRACTICE_EMAIL=your-email@example.com
SIMPLE_PRACTICE_PASSWORD=your-password

# Domain restriction (optional - restrict OAuth to specific domain)
ALLOWED_DOMAIN=yourpractice.com

# Chrome Extension Form Filler
NEXT_PUBLIC_FORM_FILLER_EXTENSION_ID=your-extension-id-from-chrome
